{
  "name": "window manager",
  "version": "1.5",
  "description": "快捷管理你的tabs",
  "permissions": ["declarativeContent", "storage", "tabs", "unlimitedStorage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "./img/folder.png"
  },
  "icons": {

    "16": "./img/folder.png",
    "32": "./img/folder.png",
    "48": "./img/folder.png",
    "128": "./img/folder.png"
  },
  "content_security_policy": {
    "script-src": "self",
    "object-src": "self"
  },
  "manifest_version": 3
}